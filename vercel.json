{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "YOUR_ENV_VARIABLE": "value"
  },
  "functions": {
    "api/*": {
      "includeFiles": "prisma/schema.prisma"
    }
  }

}